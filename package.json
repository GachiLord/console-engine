{
  "name": "console-engine",
  "version": "0.6.0",
  "description": "engine for interactive console apps",
  "repository": {
    "type": "git",
    "url": "https://github.com/GachiLord/console-engine"
  },
  "keywords": [
    "engine",
    "game",
    "console",
    "node"
  ],
  "scripts": {
    "test": "echo 'no tests'",
    "build": "npx tsc --sourceMap false",
    "startDino": "node --inspect ./dist/examples/consoleDino/index.js",
