    "@types/readline-sync": "^1.4.4",
    "bunyan": "^1.8.15",
    "mocha": "^10.2.0",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "chalk": "^5.2.0",
    "cli-cursor": "^4.0.0",
    "readline-sync": "^1.4.10"
